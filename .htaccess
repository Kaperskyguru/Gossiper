# directory browsing
Options All -Indexes
RewriteEngine On

#REGULAR PAGES
RewriteRule    ^home/?$    index.php    [NC,L]


RewriteRule    ^error/404/?$    404.php    [NC,L]


#ERRO HANDLING PAGES
ErrorDocument 400 http://localhost/wysiwyg/Not-Found
ErrorDocument 401 http://localhost/letsgossip/errors/authreqd
ErrorDocument 403 http://localhost/letsgossip/error/forbidden
ErrorDocument 404 http://localhost/letsgossip/error/404
ErrorDocument 500 http://localhost/letsgossip/error/505


